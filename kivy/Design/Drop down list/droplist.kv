<MyLayout>
    BoxLayout:
        orientation:'vertical'
        size: root.width, root.height
        Label:
            id:click_label
            text:"pick favorite pizza"
            font_size:32
        Spinner:
            id:spinner_id
            text:"Favorite"
            values:['pepperoni','cheese','mushroom']
            on_text: root.spinner_clicked(spinner_id.text)

